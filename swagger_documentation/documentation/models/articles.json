{
    "ArticleSearch": {
        "x-tag": "articles", 
        "type": "object", 
        "allOf": [
            {
                "$ref": "common.json#CommonSearch"
            }
        ], 
        "properties": {
            "resource_doi": {
                "description": "only return collections with this resource_doi", 
                "type": "string", 
                "example": "10.6084/m9.figshare.1407024"
            }, 
            "item_type": {
                "description": "Only return collections with the respective type. Mapping for item_type is: 1 - Figure, 2 - Media, 3 - Dataset, 4 - Fileset, 5 - Poster, 6 - Paper, 7 - Presentation, 8 - Thesis, 9 - Code, 11 - Metadata, 12 - Preprint", 
                "type": "string", 
                "example": 1
            }
        }
    }, 
    "PrivateArticleSearch": {
        "x-tag": "articles", 
        "type": "object", 
        "allOf": [
            {
                "$ref": "#ArticleSearch"
            }
        ], 
        "properties": {
            "resource_id": {
                "description": "only return collections with this resource_id", 
                "type": "string", 
                "example": "1407024"
            }
        }
    }, 
    "ArticleCreate": {
        "x-tag": "articles", 
        "type": "object", 
        "required": [
            "title"
        ], 
        "properties": {
            "title": {
                "type": "string", 
                "description": "Title of article", 
                "minLength": 5, 
                "example": "Test article title"
            }, 
            "description": {
                "type": "string", 
                "description": "The article description. In a publisher case, usually this is the remote article description", 
                "default": "", 
                "example": "Test description of article"
            }, 
            "tags": {
                "type": "array", 
                "description": "List of tags to be associated with the article. Keywords can be used instead", 
                "items": {
                    "type": "string"
                }, 
                "default": [], 
                "example": [
                    "tag1", 
                    "tag2"
                ]
            }, 
            "keywords": {
                "type": "array", 
                "description": "List of tags to be associated with the article. Tags can be used instead", 
                "items": {
                    "type": "string"
                }, 
                "default": [], 
                "example": [
                    "tag1", 
                    "tag2"
                ]
            }, 
            "references": {
                "type": "array", 
                "items": {
                    "type": "string", 
                    "format": "link"
                }, 
                "description": "List of links to be associated with the article (e.g [\"http://link1\", \"http://link2\", \"http://link3\"])", 
                "default": [], 
                "example": [
                    "http://figshare.com", 
                    "http://api.figshare.com"
                ]
            }, 
            "categories": {
                "type": "array", 
                "items": {
                    "type": "integer", 
                    "format": "int64"
                }, 
                "description": "List of category ids to be associated with the article(e.g [1, 23, 33, 66])", 
                "default": [], 
                "example": [
                    1, 
                    10, 
                    11
                ]
            }, 
            "authors": {
                "type": "array", 
                "items": {
                    "type": "object"
                }, 
                "description": "List of authors to be assosciated with the article. The list can contain author ids or author names. No more than 10 authors. For adding more authors use the specific authors endpoint.", 
                "default": [], 
                "example": [
                    {
                        "name": "John Doe"
                    }, 
                    {
                        "id": 1000008
                    }
                ]
            }, 
            "custom_fields": {
                "type": "object", 
                "description": "List of key, values pairs to be associated with the article", 
                "example": {
                    "defined_key": "value for it"
                }
            }, 
            "defined_type": {
                "type": "string", 
                "enum": [
                    "figure", 
                    "media", 
                    "dataset", 
                    "fileset", 
                    "poster", 
                    "paper", 
                    "presentation", 
                    "thesis", 
                    "code", 
                    "metadata"
                ],
                "description": "The response will contain an integer which is mapped to these types as: 1 - Figure, 2 - Media, 3 - Dataset, 4 - Fileset, 5 - Poster, 6 - Paper, 7 - Presentation, 8 - Thesis, 9 - Code, 11 - Metadata, 12 - Preprint",
                "example": "media"
            }, 
            "funding": {
                "type": "string", 
                "description": "Grant number or funding authority", 
                "default": ""
            }, 
            "license": {
                "type": "integer", 
                "format": "int64", 
                "description": "License id for this article.", 
                "default": 0, 
                "example": 1
            }, 
            "doi": {
                "type": "string", 
                "description": "Not appliable for regular users. In an institutional case, make sure your group supports setting DOIs. This setting is applied by figshare via opening a ticket through our support/helpdesk system.", 
                "default": ""
            }, 
            "resource_doi": {
                "type": "string", 
                "description": "Not applicable to regular users. In a publisher case, this is the publisher article DOI.", 
                "default": ""
            }, 
            "resource_title": {
                "type": "string", 
                "description": "Not applicable to regular users. In a publisher case, this is the publisher article title.", 
                "default": ""
            }
        }
    }, 
    "ArticlePresenter": {
        "x-tag": "articles", 
        "type": "object", 
        "properties": {
            "id": {
                "type": "integer", 
                "format": "int64", 
                "description": "Unique identifier for article", 
                "example": 1434614
            }, 
            "title": {
                "type": "string", 
                "description": "Title of article", 
                "example": "Test article title"
            }, 
            "doi": {
                "type": "string", 
                "description": "DOI", 
                "example": "10.6084/m9.figshare.1434614"
            }, 
            "group_id": {
                "type": [
                    "number", 
                    "null"
                ], 
                "description": "Group ID", 
                "example": 1234
            }, 
            "url": {
                "type": "string", 
                "description": "Api endpoint for article", 
                "format": "url", 
                "example": "http://api.figshare.com/articles/1434614"
            }, 
            "url_public_html": {
                "type": "string", 
                "description": "Public site endpoint for article", 
                "format": "url", 
                "example": "https://figshare.com/articles/Test_article_title/1434614"
            }, 
            "url_public_api": {
                "type": "string", 
                "description": "Public Api endpoint for article", 
                "format": "url", 
                "example": "https://api.figshare.com/articles/1434614"
            }, 
            "url_private_html": {
                "type": "string", 
                "description": "Private site endpoint for article", 
                "format": "url", 
                "example": "https://figshare.com/account/articles/1434614"
            }, 
            "url_private_api": {
                "type": "string", 
                "description": "Private Api endpoint for article", 
                "format": "url", 
                "example": "https://api.figshare.com/account/articles/1434614"
            }, 
            "published_date": {
                "type": [
                    "string", 
                    "null"
                ], 
                "description": "Publishing date", 
                "example": "2015-12-31T23:59:59.000Z"
            }, 
            "thumb": {
                "type": "string", 
                "description": "Thumbnail image", 
                "format": "url", 
                "example": "https://ndownloader.figshare.com/files/123456789/preview/12345678/thumb.png"
            }, 
            "defined_type": {
                "type": "integer",
                "format": "int64", 
                "description": "Integer which is mapped to the types as: 1 - Figure, 2 - Media, 3 - Dataset, 4 - Fileset, 5 - Poster, 6 - Paper, 7 - Presentation, 8 - Thesis, 9 - Code, 11 - Metadata, 12 - Preprint",
                "example": 3
            }
        }
    }, 
    "ArticlePresenter.L1": {
        "x-tag": "articles", 
        "type": "object", 
        "allOf": [
            {
                "$ref": "#ArticlePresenter"
            }
        ], 
        "properties": {
            "citation": {
                "type": "string", 
                "example": "lilliput, figshare admin (2017): first project item. figshare.\n \n Retrieved: 14 01, May 22, 2017 (GMT)"
            }, 
            "confidential_reason": {
                "type": "string", 
                "example": "none"
            }, 
            "embargo_type": {
                "type": [
                    "string", 
                    "null"
                ], 
                "example": null
            }, 
            "is_confidential": {
                "type": "boolean", 
                "example": true
            }, 
            "size": {
                "type": "integer", 
                "format": "int64", 
                "example": 69939
            }, 
            "funding": {
                "type": "string", 
                "example": "none"
            }, 
            "tags": {
                "type": "array", 
                "items": {
                    "type": "string"
                }, 
                "example": [
                    "t1", 
                    "t2", 
                    "t3"
                ]
            }, 
            "version": {
                "type": "integer", 
                "format": "int64", 
                "example": 1
            }, 
            "is_active": {
                "type": "boolean", 
                "example": true
            }, 
            "is_metadata_record": {
                "type": "boolean", 
                "example": false
            }, 
            "metadata_reason": {
                "type": "string", 
                "example": "hosted somewhere else"
            }, 
            "status": {
                "type": "string", 
                "example": "public"
            }, 
            "description": {
                "type": "string", 
                "example": "article description"
            }, 
            "is_embargoed": {
                "type": "boolean", 
                "example": true
            }, 
            "embargo_date": {
                "type": [
                    "string", 
                    "null"
                ], 
                "example": "2017-05-18T11:49:03Z"
            }, 
            "is_public": {
                "type": "boolean", 
                "example": true
            }, 
            "modified_date": {
                "type": [
                    "string", 
                    "null"
                ], 
                "example": "2017-05-18T11:49:03Z"
            }, 
            "created_date": {
                "type": "string", 
                "example": "2017-05-18T11:49:03Z"
            }, 
            "has_linked_file": {
                "type": "boolean", 
                "example": true
            }, 
            "categories": {
                "type": "array", 
                "items": {
                    "$ref": "common.json#CategoryPresenter"
                }
            }, 
            "license": {
                "$ref": "institutions.json#LicensePresenter"
            }, 
            "embargo_reason": {
                "type": "string", 
                "example": "not complete"
            }, 
            "references": {
                "type": "array", 
                "items": {
                    "type": "string", 
                    "format": "url"
                }, 
                "example": [
                    "http://figshare.com", 
                    "http://figshare.com/api"
                ]
            }
        }
    }, 
